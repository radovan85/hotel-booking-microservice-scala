# Routes
POST    /api/reservations/provideReservations      com.radovan.play.controllers.ReservationController.provideReservations
POST   /api/reservations/book     com.radovan.play.controllers.ReservationController.addReservation()
GET    /api/reservations/me       com.radovan.play.controllers.ReservationController.provideMyReservations
GET    /api/reservations    com.radovan.play.controllers.ReservationController.getAllReservations
GET    /api/reservations/expired    com.radovan.play.controllers.ReservationController.getAllExpiredReservations
GET    /api/reservations/active    com.radovan.play.controllers.ReservationController.getAllActiveReservations
DELETE  /api/reservations/cancel/:id   com.radovan.play.controllers.ReservationController.cancelReservation(id:Int)
DELETE  /api/reservations/:id   com.radovan.play.controllers.ReservationController.deleteReservation(id:Int)
GET  /api/reservations/:id   com.radovan.play.controllers.ReservationController.getReservationDetails(id:Int)
GET  /api/reservations/findAlternatives/:id   com.radovan.play.controllers.ReservationController.findAlternativeRooms(id:Int)
PUT  /api/reservations/:id   com.radovan.play.controllers.ReservationController.updateReservation(id:Integer)

#Notes
GET   /api/notes    com.radovan.play.controllers.NoteController.getAllNotes
GET   /api/notes/today    com.radovan.play.controllers.NoteController.getTodaysNotes
DELETE  /api/notes/removeAll   com.radovan.play.controllers.NoteController.removeAllNotes()
GET   /api/notes/:id    com.radovan.play.controllers.NoteController.getNoteDetails(id:Int)
DELETE   /api/notes/:id    com.radovan.play.controllers.NoteController.deleteNote(id:Int)


#Health
GET     /api/health     com.radovan.play.controllers.HealthController.healthCheck
